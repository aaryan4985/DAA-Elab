#include<iostream>
using namespace std;
int main(){
    string s,t;
    cin>>s>>t;
    int i,flag = 1;
    int check = int(s[0]) - int(t[0]);
    for(i=0;i<(int)s.length();i++)
    if(int(s[i]) - int(t[i])!= check)
    {flag = 0;break;}
    if(flag==0)
    cout<<"No";
    else
    cout<<"Yes";
    return 0;
}